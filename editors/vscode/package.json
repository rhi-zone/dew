{
  "name": "wick-lang",
  "displayName": "Wick Language",
  "description": "Syntax highlighting for Wick expression language",
  "version": "0.1.0",
  "publisher": "rhizome-lab",
  "repository": {
    "type": "git",
    "url": "https://github.com/rhi-zone/wick"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "wick",
        "aliases": ["Wick", "wick"],
        "extensions": [".wick"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "wick",
        "scopeName": "source.wick",
        "path": "./syntaxes/wick.tmLanguage.json"
      }
    ]
  }
}
